{
    "name":"XMC4400 Enterprise Kit",
    "target":"xmc4400_enterprisekit",
    "cc":"armgcc",
    "classes":["xmc4400_enterprisekit.XMC4400EnterpriseKit"],
    "ram":352,
    "flash":2048,
    "family_type":"armcmx",
    "family_name":"xmc4000",
    "chip_model":"xmc4400",
    "manual_reset": 0,
    "preferred_baud":115200,
    "board_producer":"Infineon",
    "chip_manifacturer":"Infineon",
    "architecture":"cortex-m4",
    "gccopts":{
        "arch": ["cortex-m4", "-mthumb"],
        "cflags": ["-mcpu=cortex-m4","-mthumb", "-O2","-fomit-frame-pointer","-falign-functions=16","-ffunction-sections", "-fdata-sections", "-fno-common", "-Wall", "-Wextra", "-Wstrict-prototypes","-nostdlib","-std=c99"],
        "defs": ["VM_DEFAULT_THREAD_SIZE=4096"]
    },
    "relocator":"cortex-m",
    "vm_chunk": 256,
    "type":"board",
    "virtualizable":"XMC4400EnterpriseKit",
    "uplink_reset": "reset",
    "jtag_target":"xmc4xxx.cfg",
    "jtag_capable": 2,
    "jtag_class":"XMC4400EnterpriseKit",
    "jtag_chipid_command": "reset halt; mdw 0x20000000; mdw 0x20000004; mdw 0x20000008; mdw 0x2000000c",
    "jtag_chipid_len":4,
    "jtag_chipid_prefix":"0x2000",
    "jtag_timeout": 180,
    "jtag_burn_end": "reset; resume",
    "jtag_target_options": "transport select swd; set CHIPNAME xmc4400",
    "bytecode_offset":"0xC020000",
    "vmstore_offset":"0xC01f000",
    "vm_start":"0xC000000",
    "connection":{
        "baudrate":115200,
        "parity":"n",
        "bytesize":8,
        "stopbits":1,
        "dsrdtr":false,
        "rtscts":false
    }
    
}
